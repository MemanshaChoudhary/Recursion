// check whether array is sorted or not
public class ArrayisSortedOrNot{
    public static void main(String args[]) {
    int a[] ={1,4,60,8,9};
    int l = a.length;
    for(int i = 0;i<l;i++)
    {
        for(int j=i+1;j<l;j++)
        {
       if(a[i]>a[j]){
           System.out.println("not sorted");
           return;
       }
    
        }
       }
       System.out.println("Sorted");

   }
}
